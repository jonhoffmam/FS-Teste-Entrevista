@model WebAtividadeEntrevista.Models.ClienteBeneficiarioModel
<div class="modal fade" id="beneficiariosModal" tabindex="-1" role="dialog" aria-labelledby="beneficiariosModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="beneficiariosModalLabel">Beneficiário</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                @Html.Partial("_FormBeneficiario", Model.Beneficiario)

                <div id="listaBeneficiarios">
                    @{ Html.RenderPartial("_ListaBeneficiarios", Model.Beneficiarios); }
                </div>

            </div>
        </div>
    </div>
</div>